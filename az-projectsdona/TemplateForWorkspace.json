			"defaultValue": "az-projectsdona"
		},
		"az-projectsdona-WorkspaceDefaultSqlServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'az-projectsdona-WorkspaceDefaultSqlServer'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=tcp:az-projectsdona.sql.azuresynapse.net,1433;Initial Catalog=@{linkedService().DBName}"
		},
		"az-projectsdona-WorkspaceDefaultStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azureprojectsdona.dfs.core.windows.net"
		}
	},
